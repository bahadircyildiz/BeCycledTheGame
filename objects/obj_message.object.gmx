            <kind>1</kind>
            <string>/// Control scale in and out
if(done == 0){
  if(scale &lt; 1){
    scale += 0.05;
  }else{
    done = 1;
  }
  exit;
}
if(global.setup == 0){exit;}
if(done == 1){   
  if(delay == 0){
    with (instance_create(x,y+80,obj_button)) {btn_txt = other.btn_txt;}
    done = 3;
  }else{
    alarm[0] = delay*room_speed;
    done = 2;
  }
  exit;
}
if(done == 3 &amp;&amp; !instance_exists(obj_button) &amp;&amp; alarm[0] == -1){
  done = 4;
}
if(done == 4){
  if(scale &gt; 0){
    scale -= 0.05;
  }else{
    window_set_cursor(cr_arrow);
    if(target_room == -1){
      instance_destroy();
    }else{
      room_goto(target_room);
    }
  }
}
</string>
