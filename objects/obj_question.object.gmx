        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Handle scale in and out
if(done == 0){
  if(scale &lt; 1){
    scale += 0.05;
  }else{
    done = 1;
    // create the buttons
    if(btn1_txt != ""){
      with (instance_create(x-110,y+60,obj_button)) {
        btn_txt = other.btn1_txt;
        result = 1;
        created_by = other.id;
      }
    }
    if(btn2_txt != ""){
      with (instance_create(x,y+60,obj_button)) {
        btn_txt = other.btn2_txt;
        result = 2;
        created_by = other.id;
      }
    }
    if(btn3_txt != ""){
      with (instance_create(x+110,y+60,obj_button)) {
        btn_txt = other.btn3_txt;
        result = 3;
        created_by = other.id;
      }
    }
  }
  exit;
}
if(done == 1 &amp;&amp; result &gt; 0){
  // destroy all buttons
  with (obj_button) {instance_destroy();}
  done = 2;
}
if(done == 2){
  if(scale &gt; 0){
    scale -= 0.05;
  }else{
    // Return the button pressed result to the created_by instance
    // (Will be 1, 2, or 3 coresponding to the button selected)
    with (created_by){pressed = other.result;}
    window_set_cursor(cr_arrow);
    instance_destroy();
  }
}
</string>
