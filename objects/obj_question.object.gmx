            <kind>1</kind>
            <string>/// Custom question box with 2 or 3 buttons
// (Similar to a Windows pop-up but does not halt execution) 
if(instance_number(obj_question) &gt; 1){
  instance_destroy(); // We only want one of these at a time.
  exit;
}
image_speed = 0;
image_index = 1; // opaque background
x = 456;
y = 360;
scale = 0;
done = 0;
active = false;
result = 0; // the button value, returns 1, 2 or 3 to the 'created_by' instance

// the following are overridden on creation
msg_txt = "6 hanelik soru alani#";
btn1_txt = "Evet";   // (Left button result = 1)
btn2_txt = "";      // (Middle button result = 2) not used if empty
btn3_txt = "Hayir";    // (Right button result = 3)
created_by = noone; // instance which created me

</string>
