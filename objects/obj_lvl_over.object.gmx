            <string>/// Message box for Level Completed / Failed
if(instance_number(obj_message) &gt; 1){
  // ensure only one message is displayed.
  instance_destroy();
  exit;
}
image_speed = 0;
image_index = 0;
// message position and scale factor
x = 456; 
y = 360;
scale = 0;
done = 0; // Stage done counter
/* Legend 
   0 = fade in
   1 = faded in but music still playing
   2 = fade out 
*/

// Any of the following can be overriden on creation
msg_txt = "6 hanelik kaybetme mesaji#";
lvl = global.level; 
failed = true;
