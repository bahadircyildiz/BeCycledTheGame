        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Control scale in and scale out
if(done == 0){
  if(scale &lt; 1){
    scale += 0.05;
  }else{
    done = 1;
  }
  exit;
}
// scale down (out)
if(done == 1 &amp;&amp; !audio_is_playing(music)){
  done = 2;
  with (instance_create(x-70,y+70,obj_button)) {btn_txt = "Devam Et";}
  with (instance_create(x+70,y+70,obj_fb_button)) {btn_txt = "Paylas";}
  exit;
}
if(done == 2 &amp;&amp; !instance_exists(obj_button)){
  if(scale &gt; 0){
    scale -= 0.05;
  }else{
    scale = 0;
    window_set_cursor(cr_arrow);
    if(!failed){
      if(lvl &gt; global.level){
        global.level = lvl;
        // Increase difficulty by increasing / changing sprites
        if(global.item_max &lt; sprite_get_number(spr_match_items)){
          global.item_max += 1;
          if(global.item_max - global.item_min &gt; 8){
            global.item_min = global.item_max-8;
          }
        }
      }
    }
    if(target_room == -1){target_room = room;}
    if(room == target_room){
      audio_sound_gain(snd_level1,1,3000);
    }
    room_goto(target_room);
  }
}
</string>
